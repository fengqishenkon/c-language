strcat函数的实现

#include<stdio.h>
char* my_strcat(char* dest, const char* src)
{
	char* p = dest;
	while (*dest != '\0')
	{
		dest++;
	}
	while (*dest = *src)
	{
		dest++; 
		src++;
	}
	return p;
}

int main()
{
	char arr1[20] = "abc";
	char arr2[] = "defg";
    my_strcat(arr1, arr2);
	printf("%s\n", arr1);
	return 0;
}
